{
  "name": "enlist",
  "version": "0.1.0",
  "description": "Prime immutable sequence",
  "main": "lib/index.js",
  "module": "es/index.js",
  "esnext": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "clean": "rimraf lib/ && rimraf es/",
    "test": "cross-env BABEL_ENV=test jest --config jest.json",
    "coverage": "cross-env BABEL_ENV=test jest --coverage",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src/ -d lib -s",
    "build:es": "babel src/ -d es -s",
    "build": "npm run build:cjs && npm run build:es",
    "watch": "npm run build:cjs -- --watch",
    "prepublish": "npm run clean && npm run build"
  },
  "author": "Zero Bias",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-closure-elimination": "^1.1.16",
    "babel-plugin-minify-dead-code-elimination": "^0.2.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-es2015-for-of": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-flow-comments": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.5.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsdoc": "^3.1.2",
    "flow-bin": "^0.53.1",
    "jest": "^20.0.4",
    "rimraf": "^2.6.1"
  },
  "files": [
    "src",
    "lib",
    "es"
  ]
